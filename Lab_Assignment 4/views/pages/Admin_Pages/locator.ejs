<h3>Outlets</h3>
<a href="/admin/locator/Outlet" class="btn btn-info">Add Outlet +</a>
<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>_id</th>
            <th>Latitude</th>
            <th>Longitude</th>
            <th>Name</th>
            <th>Address</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i=0; i < location.length ; i++ ){%>
            <tr>
                <td><%= location[i]._id %></td>
                <td><%= location[i].latitude %></td>
                <td><%= location[i].longitude %></td>
                <td><%= location[i].name %></td>
                <td><%= location[i].address %></td>
                <td>
                    <a href="/admin/locator/edit/<%= location[i]._id %>" class="btn btn-warning">Edit</a>
                    <a href="/admin/locator/delete/<%= location[i]._id %>" class="btn btn-danger">Del</a>
                </td>
            </tr>
        <% } %>
    </tbody>
</table>